{
  "name": "quara",
  "displayName": "Quara",
  "description": "",
  "publisher": "wandeber",
  "version": "0.0.3",
  "license": "ISC",
  "author": "Bernardo A. Siverio (wandeber)",
  "repository": {
    "type": "git",
    "url": "https://github.com/wandeber/vscode-quara"
  },
  "keywords": [
    "quara",
    "syntax highlighting",
    "programming language"
  ],
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "quara",
